{
  "schemaVersion": 1,
  "id": "modid",
  "version": "${version}",

  "name": "Shield Example Mod",
  "description": "This is an example of a mod made using Fabric Shield Lib!",
  "authors": [
    "Me!"
  ],
  "contact": {
    "homepage": "www.curseforge.com/minecraft/mc-mods/fabric-shield-lib",
    "sources": "https://github.com/CrimsonDawn45/Fabric-Shield-Lib-Example-Mod",
    "issues": "https://github.com/CrimsonDawn45/Fabric-Shield-Lib-Example-Mod/issues"
  },

  "license": "CC0-1.0",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.fabricmc.example.ExampleMod"
    ],
    "client": [
      "net.fabricmc.example.ExampleModClient"
    ]
  },
  "mixins": [
    "modid.mixins.json"
  ],

  "depends": {
    "fabricshieldlib": ">=1.6.0-1.19",
    "fabricloader": ">=0.14.9",
    "fabric-api": "*",
    "minecraft": "~1.19",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
